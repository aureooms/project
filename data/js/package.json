{{
  "name": "{fullname}",
  "description": "{description}",
  "version": "0.0.0",
  "author": "{author}",
  "ava": {{
    "require": [
      "babel-polyfill",
      "babel-register"
    ]
  }},
  "babel": {{
    "presets": [
      "env"
    ],
    "env": {{
      "development": {{
        "sourceMaps": "inline"
      }}
    }}
  }},
  "bugs": {{
    "url": "https://github.com/{repository}/issues"
  }},
  "dependencies": {{}},
  "devDependencies": {{
    "ava": "^0.23.0",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^3.0.0",
    "esdoc": "^1.0.3",
    "esdoc-inject-script-plugin": "^1.0.0",
    "esdoc-inject-style-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "np": "^2.18.3",
    "nyc": "^11.2.1"
  }},
  "homepage": "{homepage}",
  "keywords": [{keywords}],
  "license": "{license}",
  "main": "lib/index.js",
  "repository": {{
    "type": "git",
    "url": "https://github.com/{repository}"
  }},
  "scripts": {{
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepublish": "npm run build",
    "release": "np",
    "test": "ava ./test/src"
  }}
}}
