    "ava": "^0.15.2",
    "babel-cli": "^6.5.1",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "codeclimate-test-reporter": "^0.3.1",
    "coveralls": "^2.11.6",
    "esdoc": "^0.4.5",
    "nyc": "^7.0.0"
  }},
  "ava": {{
    "require": [
      "babel-register"
    ]
  }},
  "babel": {{
    "presets": [
